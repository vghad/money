{
    "name": "phpab/phpab",
    "description": "A PHP library to create A/B tests.",
    "keywords": ["ab", "abtesting", "test", "testing"],
    "license": "MIT",
    "require": {
        "php": ">=5.3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5"
    },
    "autoload": {
        "psr-4": {
            "PhpAb\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhpAbTest\\": "tests/PhpAbTest",
            "PhpAbTestAsset\\": "tests/PhpAbTestAsset"
        }
    },
    "authors": [
        {
            "name": "Walter Tamboer",
            "homepage": "http://www.waltertamboer.nl",
            "role": "Developer"
        }
    ]
}
